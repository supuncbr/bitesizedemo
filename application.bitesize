project: bitesizedemo
applications:
  - name: nodejs-app
    runtime: ubuntu-nodejs5x:1.2
    version: "1.0.0"
    dependencies:
      - name: nodejs-app
        type: debian-package
        origin:
          build: nodejs-app
        version: 1.0
    command: "npm start --prefix /app"
  - name: php-app
    runtime: ubuntu-php-fpm:1.7
    version: "1.0.0"
    command: "ls /etc/php5/fpm/; php5-fpm -F --fpm-config /etc/php5/fpm/php-fpm.conf"
    
